					expect(
						GeneratePBKDFKey( 'PBKDF2WithHmacSHA1' , passphrase , salt, iterations , keysize )
					).toBe(
						''
					);
				});

				it( 'for PBKDF2WithHmacSHA1 without optional optional arguments' , function() {
